        if (event === 'pull_request' || event === 'pull_request_target') {
            issueNumber = context.payload.pull_request.number;
        }
        else if (event === 'issues') {
            issueNumber = context.payload.issue.number;
        }

        const octokit = github.getOctokit(github_token);

